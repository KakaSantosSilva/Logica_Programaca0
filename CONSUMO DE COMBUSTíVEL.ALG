Algoritmo "Consumo de combustível"

Var

km, litros, consumo :  real


Inicio

//ENTRADA

Escreval("Informe quantos km o carro rodou: ")
leia(km)

Escreval("informa quantos litros enche o tanque")
leia(litros)


//PROCESSAMENTO

 consumo <- km / litros
 
 
 //SAÍDA
 
 
 Escreval(" O consumo é de : ", consumo)





Fimalgoritmo